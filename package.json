{
  "name": "logseq-solarized-extended-theme",
  "description": "Solarized Logseq theme with extra stuff",
  "version": "2.6.0",
  "author": "yoyurec <yoyurec@gmail.com>",
  "main": "dist/index.html",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && cp -f -r src/css dist/assets && cp -f -r src/vendors dist/vendors",
    "preinstall": "npx only-allow pnpm",
    "semantic-release": "semantic-release"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@logseq/libs": "0.0.8",
    "@simonwep/pickr": "^1.8.2",
    "color2k": "^2.0.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "conventional-changelog-conventionalcommits": "^5.0.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^8.24.0",
    "semantic-release": "^19.0.5",
    "typescript": "^4.8.4",
    "vite": "3.1.0",
    "vite-plugin-logseq": "^1.1.2"
  },
  "license": "MIT",
  "logseq": {
    "title": "Solarized Ext: Plugin (UI, features) + Theme(colors)",
    "icon": "./icon.png",
    "id": "logseq-solarized-extended-theme"
  },
  "repository": "https://github.com/yoyurec/logseq-solarized-extended-theme.git",
  "effect": true,
  "sponsors": [
    "https://www.buymeacoffee.com/yoyurec"
  ]
